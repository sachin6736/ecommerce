	<header>
		<!-- TOP HEADER -->
		<div id="top-header">
			<div class="container">

				<ul class="header-links pull-right">

					<% if (isAuth) { %>
						<li><a href="/user_Account"><i class="fa fa-user-o"></i>
								<%= USER.firstname %>
							</a></li>
						<a href="/logout_user">Logout</a>
						<% }else {%>

